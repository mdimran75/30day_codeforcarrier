#include<stdio.h>
#include<string.h>
int main()
{
    char s[]="hyderabd-uppal-kalyanipuri";
    char* token;
    token=strtok(s,"-");
    while(token!=NULL)
    {
        printf("%s\n",token);
        token=strtok(NULL,"-");
    }
}




